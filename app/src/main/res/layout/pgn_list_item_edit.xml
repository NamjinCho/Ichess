<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/relativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <ImageView
        android:layout_height="50dp"
        android:id="@+id/list_item_img"
        android:layout_width="288dp"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/btn_white">
    </ImageView>

    <TextView
        android:text="TextView"
        android:layout_height="wrap_content"
        android:id="@+id/list_item_title"
        android:gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_marginLeft="15dp"
        android:textStyle="bold"
        android:textSize="12sp"
        android:textColor="@color/pgn"
        android:layout_alignTop="@+id/list_item_img"
        android:layout_alignBottom="@+id/list_item_img"></TextView>

    <ImageView
        android:layout_width="288dp"
        android:layout_height="50dp"
        android:src="@drawable/bg_make_black"
        android:scaleType="fitXY"
        android:id="@+id/imageView34" />
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/frameLayout4"
        android:onClick="EditButton"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="13dp"
        android:layout_alignTop="@+id/list_item_img"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:id="@+id/imageView32"
            android:src="@drawable/icon_write_01_blue"
            android:layout_alignBottom="@+id/frameLayout4"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13sp"
            android:textStyle="bold"
            android:textColor="#ffffff"
            android:text="수정"
            android:layout_alignParentStart="true"
            android:id="@+id/textView28"
            android:layout_marginLeft="25dp" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/frameLayout5"
        android:onClick="DeleteButton"
        android:layout_alignTop="@+id/frameLayout4"
        android:layout_alignEnd="@+id/list_item_img"
        android:layout_marginEnd="51dp"
        android:layout_marginRight="50dp">

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:id="@+id/imageView31"
            android:src="@drawable/icon_write_02_blue"
            android:layout_alignBottom="@+id/frameLayout4"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13sp"
            android:textStyle="bold"
            android:textColor="#ffffff"
            android:text="삭제"
            android:layout_alignParentStart="true"
            android:id="@+id/textView29"
            android:layout_marginLeft="25dp" />
    </FrameLayout>
</RelativeLayout>
